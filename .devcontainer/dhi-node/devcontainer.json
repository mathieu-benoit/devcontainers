{
    "name": "Node.js & TypeScript (DHI)",
    "dockerComposeFile": "compose.yaml",
    "service": "workspace",
    "workspaceFolder": "/workspace",
	"remoteUser": "node",
	"containerEnv": {
		"SHELL": "/bin/bash",
		"DOCKER_USERNAME": "${localEnv:DOCKER_USERNAME}",
		"DOCKER_PASSWORD": "${localEnv:DOCKER_PASSWORD}"
	}
	//"initializeCommand": "echo ${DOCKER_PASSWORD} | docker login -u ${DOCKER_USERNAME} --password-stdin",
}
